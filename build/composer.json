{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true,
      "phpstan/extension-installer": true,
      "oomphinc/composer-installers-extender": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "description": "Drupal component of slaveryandfreedomlaws.lib.unb.ca",
  "extra": {
    "enable-patching": true,
    "drupal-scaffold": {
      "locations": {
        "web-root": "./"
      }
    },
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "libraries/ckeditor/plugins/{$name}": [
        "vendor:ckeditor-plugin"
      ],
      "libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "patches": {
      "drupal/core": {
        "Fix form state notice issue 2700667": "https://www.drupal.org/files/issues/2021-05-12/2700667-133a.patch"
      }
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/slaveryandfreedomlaws.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip"
        },
        "name": "harvesthq/chosen",
        "type": "drupal-library",
        "version": "1.8.7"
      },
      "type": "package"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/furf/jquery-ui-touch-punch/archive/refs/heads/master.zip"
        },
        "name": "furf/jquery-ui-touch-punch",
        "type": "drupal-library",
        "version": "dev-master"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/simeydotme/jQuery-ui-Slider-Pips/archive/v1.11.4.zip"
        },
        "name": "simeydotme/jquery-ui-slider-pips",
        "require": {
          "composer/installers": "^2"
        },
        "type": "drupal-library",
        "version": "1.11.4"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://download.ckeditor.com/widget/releases/widget_4.16.1.zip"
        },
        "name": "ckeditor-plugin/widget",
        "require": {
          "composer/installers": "^2"
        },
        "type": "drupal-library",
        "version": "4.16.1"
      },
      "type": "package"
    }
  ],
  "require": {
    "ckeditor-plugin/widget": "^4",
    "dompdf/dompdf": "^2",
    "drupal/address": "^1",
    "drupal/admin_toolbar": "^3",
    "drupal/bootstrap4": "^3",
    "drupal/bootstrap5": "^3",
    "drupal/chosen": "^3",
    "drupal/colorbox": "^2",
    "drupal/config_filter": "^2",
    "drupal/config_ignore": "^2",
    "drupal/core-composer-scaffold": "^10",
    "drupal/core-recommended": "^10",
    "drupal/core": "^10",
    "drupal/easy_breadcrumb": "^2",
    "drupal/entity_reference_revisions": "^1",
    "drupal/facets": "^2",
    "drupal/fontawesome": "^2",
    "drupal/geocoder": "^4",
    "drupal/geocoder_ajax_prepopulate": "^3",
    "drupal/geofield": "^1",
    "drupal/geofield_map": "^2",
    "drupal/google_analytics": "^4",
    "drupal/health_check": "^1",
    "drupal/imagemagick": "^3",
    "drupal/inline_entity_form": "^1",
    "drupal/jquery_ui": "^1",
    "drupal/jquery_ui_slider": "^2",
    "drupal/jquery_ui_touch_punch": "^1",
    "drupal/leaflet": "^10",
    "drupal/leaflet_more_maps": "^2",
    "drupal/views_data_export": "^1",
    "drupal/mgv": "^2",
    "drupal/node_edit_protection": "^1",
    "drupal/pathauto": "^1",
    "drupal/pdfpreview": "^1",
    "drupal/redis": "^1",
    "drupal/samlauth": "^3",
    "drupal/search_api": "^1",
    "drupal/search_api_solr": "^4",
    "drupal/title_length": "^2",
    "drupal/token": "^1",
    "drupal/twig_field_value": "^2",
    "drupal/twig_tweak": "^3",
    "drupal/views_timelinejs": "^4",
    "furf/jquery-ui-touch-punch": "dev-master",
    "geocoder-php/nominatim-provider": "^5",
    "harvesthq/chosen": "^1",
    "mikehaertl/phpwkhtmltopdf": "^2",
    "oomphinc/composer-installers-extender": "^2",
    "simeydotme/jquery-ui-slider-pips": "^1",
    "tecnickcom/tcpdf": "^6",
    "unb-libraries/saml_features": "dev-10.x-1.x",
    "unb-libraries/composer-drupal-scaffold": "^10"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^10"
  },
  "type": "project"
}
